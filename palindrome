import java.lang.*;
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int no,n,rev=0;
	    Scanner sc= new Scanner(System.in);
	    n=sc.nextInt();
	    no=n;
	    while(n!=0)
	    {
	      int r=n%10;
	      rev=rev*10+r;
	      n=n/10;
	    }
	    if(rev == no){
	        System.out.println("palindrome");
	    }
	    else{
	        System.out.println("not palindrome");
	    }
		
	}
}
