import java.lang.*;
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int n=0,m=1,b;
        int a;
        Scanner sc= new Scanner(System.in);
        a=sc.nextInt();
        System.out.println(n);
        System.out.println(m);
        for(int i=2;i<=a;i++){
            b=n+m;    
            System.out.println(b);    
            n=m;    
            m=b;
        }
    }
}
