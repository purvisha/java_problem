import java.util.*;
public class Main
{
	public static void main(String[] args) {
		
		Scanner s= new Scanner(System.in);
		int a=s.nextInt();
		int z=a,sum=0;
		int b=a*a;
		while(b!=0){
		    int q=b%10;
		    sum+=q;
		    b=b/10;
		}
		if(sum==z){
		    System.out.println("neon number");
		}
		else{
		    System.out.println("not a neon number");
		}
		
	}
}
