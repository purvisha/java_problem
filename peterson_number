import java.lang.*;
import java.util.*;

public class Main
{
	public static void main(String[] args) {
		int a,b,c;
		Scanner sc=new Scanner(System.in);
		a=sc.nextInt();
		b=a;
		int sum=0;
		
		while(a!=0)
		{
		    
		    int r=a%10;
		    int p=1;
		    for(int i=1;i<=r;i++){
		        p=p*i;
		         
		    }
		    sum+=p;
		     
		    a=a/10;
		    
		}
		if(sum==b){
		    System.out.println("peterson number");
		}
		else{
		    System.out.println("not peterson number");
		}
	}
}
